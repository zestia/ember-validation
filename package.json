{
  "author": "Zestia",
  "dependencies": {
    "@babel/core": "^7.22.20",
    "date-fns": "^2.30.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^8.0.0",
    "ember-cli-htmlbars": "^6.3.0"
  },
  "description": "Simple validation utilities for Ember apps",
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@zestia/eslint-config": "^5.1.4",
    "@zestia/prettier-config": "^1.3.1",
    "@zestia/stylelint-config": "^5.2.0",
    "@zestia/template-lint-config": "^6.0.7",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.1",
    "ember-cli": "^5.3.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^4.12.3",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "^5.3.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-n": "^16.1.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.0.3",
    "qunit": "^2.19.4",
    "qunit-dom": "^3.0.0",
    "release-it": "^16.2.1",
    "sass": "^1.69.4",
    "stylelint": "^15.10.3",
    "webpack": "^5.88.2"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://zestia.github.io/ember-validation"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "keywords": [
    "ember-addon",
    "validation",
    "validator"
  ],
  "license": "MIT",
  "name": "@zestia/ember-validation",
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-validation.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prettier": "prettier . --write",
    "release": "release-it",
    "start": "ember serve",
    "test": "concurrently --kill-others-on-fail \"npm:lint\" \"npm:test:ember\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "version": "5.0.0"
}
